{
    "stages": [
        {
            "id": 1,
            "name": "Personal information",
            "description": "RocketRoute would like to give you the best product. This survey will take few seconds.",
            "questions": [
                {
                    "id": 0,
                    "header": "name",
                    "text": "My name is ***",
                    "input": {
                        "type": "text",
                        "val_type": "name // length: 3­255"
                    }
                },
                {
                    "id": 1,
                    "header": "email",
                    "text": "I’m using *** email",
                    "input": {
                        "type": "email",
                        "val_type": "email // String, email valid"
                    }
                },
                {
                    "id": 2,
                    "header": "phone",
                    "text": "And my phone number is ***",
                    "input": {
                        "type": "tel",
                        "val_type": "tel // Phone input, Valid US phone number, optional"
                    }
                }
            ]
        },
        {
            "id": 2,
            "name": "Address",
            "description": "We need your address to confirm our best offer",
            "questions": [
                {
                    "id": 0,
                    "header": "country",
                    "text": "I live in ***",
                    "input": {
                        "type": "______________",
                        "val_type": "country // Select with search, Country name"
                    }
                },
                {
                    "id": 1,
                    "header": "zip",
                    "text": "My zip code is ***",
                    "input": {
                        "type": "number",
                        "val_type": "zip // String???, Valid zip code"
                    }
                }
            ]
        },
        {
            "id": 3,
            "name": "General information",
            "description": "These answers provides deeper insights to craft something special",
            "questions": [
                {
                    "id": 0,
                    "header": "income",
                    "text": "My family makes $*** per year",
                    "input": {
                        "type": "number",
                        "val_type": "income // Number bigger then 0"
                    }
                },
                {
                    "id": 1,
                    "header": "family members",
                    "text": "There is *** person in my tax household",
                    "input": {
                        "type": "number",
                        "val_type": "family // Select, numbers from 1 up to 10"
                    }
                }
            ]
        }
    ],
    "finish": {
        "description": "Thanks for your answers. We will get back to you within 24 hours with our amazing product"
    }
}